mdhn:hasPublished a owl:ObjectProperty ;
    rdfs:comment "Links a publisher to DigitalResource." ;
    rdfs:domain mdhn:Publisher ;
    rdfs:label "has Published" ;
    rdfs:range mdhn:DigitalResource ;
